# This file is for unifying the coding style for different editors and IDEs
# See http://editorconfig.org/ for more information

root = true

[*]
indent_style = tab
indent_size = 4
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
end_of_line = lf

[*.json]
indent_style = space
indent_size = 2
